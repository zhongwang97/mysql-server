    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug mysqld",
            "program": "${workspaceFolder}/debug/build/bin/mysqld",
            "args": [
                "--defaults-file=${workspaceFolder}/debug/etc/RR.cnf", "--general-log=ON"
            ],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "build mysqld debug"
        },
    ]
}